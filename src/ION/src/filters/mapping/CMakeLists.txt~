cmake_minimum_required(VERSION 2.8.3)
project(mapping)

set( MAIN
       src/C_GlobalMap/GlobalMap.h
       src/C_GlobalMap/GlobalMap.cpp 
       src/C_Localization/Localization.h
       src/C_Localization/Localization.cpp
       src/names.h
)

include_directories(
  ${catkin_INCLUDE_DIRS}
)

add_executable(mapping_node ${MAIN} src/main.cpp)
target_link_libraries(listener ${catkin_LIBRARIES})

